get_filename_component(DynOSAMOdom_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${DynOSAMOdom_CMAKE_DIR})

find_dependency(Boost REQUIRED)
if(NOT TARGET Boost::boost)
  add_library(Boost::boost INTERFACE IMPORTED)
  set_target_properties(Boost::boost PROPERTIES
  INTERFACE_LINK_LIBRARIES "${Boost_LIBRARIES}"
  INTERFACE_INCLUDE_DIRECTORIES "${Boost_INCLUDE_DIRS}")
endif()
find_dependency(Gflags REQUIRED)
find_dependency(Glog REQUIRED)
find_dependency(GTSAM REQUIRED)
find_dependency(OpenCV REQUIRED)
find_dependency(opengv REQUIRED)
find_dependency(DBoW2 REQUIRED)

# We should set those right?
#find_dependency(Thread REQUIRED)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET dynosam_odom)
  include("${DynOSAMOdom_CMAKE_DIR}/dynosam_odomTargets.cmake")
endif()
